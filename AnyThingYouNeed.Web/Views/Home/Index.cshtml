<script>
    // Function to handle smooth scrolling to a target element
    function scrollToElement(elementId) {
        var targetElement = document.getElementById(elementId);
        if (targetElement) {
            targetElement.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }
    }

    // Event listener for the "Explore Our Services" button
    document.addEventListener('DOMContentLoaded', function () {
        var exploreButton = document.querySelector('.btn-explore-services');
        if (exploreButton) {
            exploreButton.addEventListener('click', function (event) {
                event.preventDefault(); // Prevent default anchor behavior
                scrollToElement('portfolio'); // Scroll to the 'services' section
            });
        }
    });
</script>